{
  "name": "nuxt-playground",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "content:dev": "nuxt --config-file src/content/nuxt.config.js -p=3000",
    "header:dev": "nuxt --config-file src/header/nuxt.config.js -p=3001",
    "content:build": "nuxt build --config-file src/content/nuxt.config.js",
    "header:build": "nuxt build --config-file src/header/nuxt.config.js",
    "content:start": "nuxt start --config-file src/content/nuxt.config.js -p=3000",
    "header:start": "nuxt start --config-file src/header/nuxt.config.js -p=3001",
    "build": "yarn header:build && yarn content:build",
    "dev": "concurrently \"yarn content:dev\" \"yarn header:dev\"",
    "start": "concurrently \"yarn content:start\" \"yarn header:start\"",
    "express": "node server.js"
  },
  "dependencies": {
    "concurrently": "^6.0.0",
    "core-js": "^3.8.3",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "nuxt": "^2.14.12"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.5.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0"
  }
}
